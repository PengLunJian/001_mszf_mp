page,
view,
icon,
text,
label,
image {
  margin: 0;
  padding: 0;
  box-sizing: border-box;
  -webkit-tap-highlight-color: transparent;
  -webkit-tap-highlight-color: rgba(0, 0, 0, 0);
}
view {
  display: block;
}
.row {
  display: flex;
  flex-wrap: wrap;
}
.col-3 {
  width: 25%;
}
.col-4 {
  width: 33.333%;
}
.col-6 {
  width: 50%;
}
.col-12 {
  width: 100%;
}
image {
  display: block;
}
page {
  font-size: 26rpx;
  font-family: "Helvetica Neue", Helvetica, Arial, sans-serif;
  background-color: #fafafa;
}
button {
  padding: 0;
  border: none;
  border-radius: 0;
  padding-right: 0;
  background-color: transparent;
}
button:focus {
  outline: none;
}
button:after {
  display: none;
}
.module {
  position: relative;
  margin-top: 20rpx;
  background-color: #ffffff;
}
.btn {
  position: relative;
}
.btn:before {
  content: '';
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  z-index: 1;
  visibility: hidden;
  transition: all 150ms ease;
}
.btn:active:before {
  visibility: visible;
  background-color: rgba(0, 0, 0, 0.3);
}
.ellipsis {
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}
.fade {
  animation: fade 300ms ease-out;
}
.fade.hide {
  animation: hide 300ms ease-out forwards;
}
@keyframes fade {
  0% {
    opacity: 0;
    visibility: hidden;
  }
  100% {
    opacity: 1;
    visibility: visible;
  }
}
@keyframes hide {
  0% {
    opacity: 1;
    visibility: visible;
  }
  100% {
    opacity: 0;
    visibility: hidden;
  }
}
@keyframes rotate {
  0% {
    transform: rotate(0deg);
  }
  50% {
    transform: rotate(180deg);
  }
  100% {
    transform: rotate(360deg);
  }
}
.module {
  margin: 0;
}
.module .module-content .module-header {
  padding: 30rpx 30rpx 0;
}
.module .module-content .module-header .module-title {
  height: 32rpx;
  line-height: 32rpx;
  padding-left: 20rpx;
  border-left: 4px solid #FF9900;
  font-size: 32rpx;
  letter-spacing: 0.5px;
  font-weight: bold;
  color: #333333;
}
.module .module-content .module-body .module-context .module-banner .banner-swiper {
  position: relative;
}
.module .module-content .module-body .module-context .module-banner .banner-swiper .swiper {
  height: 520rpx;
}
.module .module-content .module-body .module-context .module-banner .banner-swiper .swiper .swiper-img {
  width: 100%;
  height: 100%;
}
.module .module-content .module-body .module-context .module-banner .banner-code {
  width: 100%;
  position: absolute;
  left: 0;
  bottom: 30rpx;
  text-align: center;
  font-size: 0;
}
.module .module-content .module-body .module-context .module-banner .banner-code .banner-label {
  height: 44rpx;
  line-height: 44rpx;
  min-width: 90rpx;
  display: inline-block;
  padding: 0 20rpx;
  border-radius: 44rpx;
  background-color: rgba(0, 0, 0, 0.5);
  font-size: 24rpx;
  color: #ffffff;
}
.module .module-content .module-body .module-context .module-banner .banner-btns {
  display: flex;
  position: absolute;
  right: 0;
  z-index: 10;
  bottom: -60rpx;
  padding: 0 20rpx;
}
.module .module-content .module-body .module-context .module-banner .banner-btns button {
  width: 90rpx;
  border-radius: 100%;
  overflow: hidden;
  background-color: #ffffff;
  color: #999999;
}
.module .module-content .module-body .module-context .module-banner .banner-btns button .btn-icon {
  display: block;
  height: 60rpx;
  line-height: 70rpx;
  font-size: 40rpx;
}
.module .module-content .module-body .module-context .module-banner .banner-btns button .btn-text {
  display: block;
  height: 30rpx;
  line-height: 30rpx;
  font-size: 22rpx;
}
.module .module-content .module-body .module-context .module-banner .banner-btns button.btn-favorite {
  margin-right: 10rpx;
}
.module .module-content .module-body .module-context .module-banner .banner-btns button.btn-favorite .btn-icon {
  color: #F40505;
}
.module .module-content .module-body .module-context .module-banner .banner-btns button.btn-favorite .btn-icon:before {
  content: '\e633';
}
.module .module-content .module-body .module-context .module-banner .banner-btns button.btn-favorite .btn-icon.active:before {
  content: '\e63c';
}
.module .module-content .module-body .module-context .module-banner .banner-btns button.btn-forward .btn-icon {
  color: #30C404;
}

